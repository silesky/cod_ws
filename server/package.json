{
  "version": "0.0.1",
  "devDependencies": {
    "@types/node": "^10.14.10",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1",
    "tslib": "~1.10.0",
    "tslint": "~5.18.0",
    "tslint-microsoft-contrib": "~6.2.0",
    "tsutils": "~3.14.0",
    "typescript": "~3.5.0"
  },
  "scripts": {
    "dev": "npm run build && concurrently 'npm run build:watch' 'npm run start'",
    "start": "nodemon --config nodemon.json ./build/src/main.js",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "npm run build -- -w"
  }
}
